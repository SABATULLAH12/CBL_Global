body, html, .mainLayoutContainer {
    min-width: 1366px;
    min-height: 625px;
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
    position: relative;
    top: 0;
    left: 0;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}

.sprit-img {
    background-image: url('../Images/CBL Global_sprite_V10.png');
}

@font-face {
    font-family: 'Montserrat-Light';
    src: url('../../fonts/Montserrat/Montserrat-Light.ttf') format('truetype');
}

@font-face {
    font-family: 'Montserrat-Bold';
    src: url('../../fonts/Montserrat/Montserrat-Bold.ttf') format('truetype');
}

@font-face {
    font-family: 'Montserrat-Regular';
    src: url('../../fonts/Montserrat/Montserrat-Regular.ttf') format('truetype');
}

@font-face {
    font-family: 'Montserrat-Medium';
    src: url('../../fonts/Montserrat/Montserrat-Medium.ttf') format('truetype');
}

.masterHeader {
    width: 98%;
    height: 52px;
    float: left;
    margin-left: 1%;
}

.mainLayoutContainer {
    background: url('../Images/CBL Global_BG.jpg') no-repeat;
    background-size: 100% 100%;
}

.masterBody {
    width: 98%;
    margin-left: 1%;
    height: calc(100% - 18px);
    /*background: #F3F4F7;*/
    background: url('../Images/CBL Global_table bg.jpg') no-repeat;
    background-size: 100% 100%;
    float: left;
}

.header-sub-menu {
    width: 98%;
    margin-left: 1%;
    height: 55px;
    background: #303030;
    float: left;
}

.selection-summary-Div {
    background: #CCCCCC;
    height: 19px;
    width: 98%;
    margin-left: 1%;
    float: left;
    position: relative;
    /*z-index:0;*/
    font-family: Montserrat-Light;
    font-size: 12px;
    color: #212121;
    cursor: pointer;
}

    .selection-summary-Div ul {
        display: inline-block;
        width: 99%;
        position: absolute;
        margin: 0;
        padding: 0;
        background: #CCCCCC;
        z-index: 10000000;
        list-style: none;
        height: 100%;
        margin-left: 10px;
        overflow: hidden;
    }


        .selection-summary-Div ul li {
            width: auto;
            float: left;
            margin-right: 5px;
        }

        .selection-summary-Div ul .selection-filter-header {
            font-size: 12px;
        }

.selection-filter {
    font-family: Montserrat-Bold;
}

.footer {
    width: 98%;
    height: 18px;
    float: left;
    font-family: Montserrat-Light;
    font-size: 0.625em;
    color: #FFFFFF;
    margin-left: 1%;
}

.left-footer-div {
    display: inline-block;
    width: 37%;
    float: left;
    line-height: 18px;
}

.middle-footer-div {
    display: inline-block;
    float: left;
    width: 20%;
    text-align: center;
    line-height: 18px;
}

.right-footer-div {
    display: inline-block;
    float: right;
    width: 35%;
    line-height: 18px;
}

.footer span {
    margin-left: 3px;
}

.footer div {
    display: inline-block;
}

.green-footer-icon {
    background: url('../Images/CBL Global_sprite_V4.png') no-repeat -309px -305px;
    width: 8px;
    height: 8px;
}

.red-footer-icon {
    background: url('../Images/CBL Global_sprite_V4.png') no-repeat -347px -305px;
    width: 8px;
    height: 8px;
    margin-left: 12px;
}

.export-div {
    width: 100%;
    height: calc(100% - 126px);
    float: left;
}

.logo {
    float: left;
    width: 26%;
    height: 96%;
    display: flex;
    align-items: center;
    /*margin-left:1%;*/
}

.left-panel-Icon {
    float: left;
    background: url('../Images/CBL Global_sprite_V2.png') no-repeat -773px -2px;
    width: 52px;
    height: 55px;
    cursor: pointer !important;
}

.cokeLogoImg {
    float: left;
    background: url('../Images/CBL Global_LoginLanding_SpriteV2.png') no-repeat -10px -236px;
    width: 81px;
    height: 26px;
    margin-left: 1%;
}

.toolLogoImg {
    background: url('../Images/CBL Global_LoginLanding_SpriteV2.png') no-repeat -119px -236px;
    width: 192px;
    height: 42px;
    float: left;
    margin-left: 4%;
}

.separatorImg {
    background: url('../Images/CBL Global_LoginLanding_SpriteV2.png') no-repeat -223px -163px;
    width: 13px;
    height: 28px;
    float: left;
    margin-left: 6%;
}

#settings-div {
    float: right;
    width: 45px;
    height: 51px;
    cursor: pointer;
}

    #settings-div:hover {
        background: url('../Images/CBL Global_sprite_V4.png') no-repeat -1130px -1px;
    }

.active-settings {
    background: url('../Images/CBL Global_sprite_V4.png') no-repeat -1130px -1px;
}

.inactive-settings {
    background: url('../Images/CBL Global_sprite_V2.png') no-repeat -699px -1px;
}

#home-div {
    float: right;
    margin-right: 1%;
    cursor: pointer;
    background: url('../Images/CBL Global_sprite_V2.png') no-repeat -624px -1px;
    width: 46px;
    height: 51px;
}

    #home-div:hover {
        background: url('../Images/CBL Global_sprite_V4.png') no-repeat -1055px -1px;
    }

.menu-div {
    width: 31%;
    height: 100%;
    float: left;
    margin-left: 10%;
}

.menu {
    float: left;
    width: 201px;
    height: 100%;
    font-family: Montserrat-Regular;
    font-size: 0.87em;
    text-transform: uppercase;
    color: #AFAFAF;
    text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.75);
    line-height: 52px;
    text-align: center;
    cursor: pointer;
}

.separator-img {
    float: left;
    background: black;
    height: 11px;
    margin-top: 21px;
    width: 2px;
}

.selected-menu {
    background: url('../Images/CBL Global_sprite_V2.png') no-repeat -44px -98px;
    /*width: 201px;*/
    height: 100%;
    width: 201px;
    font-family: Montserrat-Medium;
    font-size: 0.938em;
    text-transform: uppercase;
    color: #FFFFFF;
    text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.75);
    cursor: default;
    pointer-events: none;
}

.sub-menu-div {
    float: left;
    width: 92%;
    height: 100%;
}

.sub-menu {
    float: left;
    width: 208px;
    height: 100%;
    font-family: Montserrat-Light;
    color: #B2B2B2;
    text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.5);
    cursor: pointer;
    box-shadow: 5px 0px 6px -6px black;
}

.selected-sub-menu {
    background: linear-gradient(to bottom, rgba(234,31,41,1) 0%, rgba(234,31,41,1) 16%, rgba(133,17,25,1) 100%);
    font-family: Montserrat-Medium;
    cursor: default;
    color: #FFFFFF;
    text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.5);
    pointer-events: none;
}

.sub-menu-img {
    float: left;
    display: flex;
    align-items: center;
    margin-top: 5px;
    margin-left: 8px;
}

.sub-menu-arrow {
    float: right;
    width: 8px;
    height: 8px;
    margin-right: 3px;
    margin-top: 44px;
}

.inactiveArrow {
    background: url('../Images/CBL Global_sprite_V2.png') no-repeat -548px -86px;
}

.activeArrow {
    background: url('../Images/CBL Global_sprite_V2.png') no-repeat -511px -86px;
}

.InactiveFiveWProfileMenuImg {
    background: url('../Images/CBL Global_sprite_V2.png') no-repeat -66px -172px;
    width: 49px;
    height: 47px;
}

.InactiveDrinkingMomentsMenuImg {
    background: url('../Images/CBL Global_sprite_V2.png') no-repeat -177px -171px;
    width: 44px;
    height: 47px;
}

.InactiveBrandFitToolMenuImg {
    background: url('../Images/CBL Global_sprite_V2.png') no-repeat -284px -171px;
    width: 44px;
    height: 46px;
}

.ActiveFiveWProfileMenuImg {
    background: url('../Images/CBL Global_sprite_V2.png') no-repeat -7px -172px;
    width: 49px;
    height: 47px;
}

.ActiveDrinkingMomentsMenuImg {
    background: url('../Images/CBL Global_sprite_V2.png') no-repeat -124px -171px;
    width: 44px;
    height: 47px;
}

.ActiveBrandFitToolMenuImg {
    background: url('../Images/CBL Global_sprite_V2.png') no-repeat -230px -171px;
    width: 44px;
    height: 46px;
}

.sub-menu-text {
    font-size: 0.81em;
    float: left;
    height: 60%;
    width: calc(100% - 75px);
    margin-top: 10px;
    line-height: 18px;
    margin-left: 5px;
    display: flex;
    align-items: center;
}

.settings-arrow {
    background: url('../Images/CBL Global_sprite_V4.png') no-repeat -1204px 0;
    width: 13px;
    height: 7px;
    float: right;
    margin-right: 12px;
    margin-top: -7px;
}

.settings-container {
    background: #000000;
    width: 9.688em;
    height: 7.625em;
    border-radius: 0px 0px 3px 3px;
    border-top: 1px Solid #EA1F2A;
}

.settings-expand-div {
    position: absolute;
    top: 51px;
    right: 20px;
    display: none;
    z-index: 10000000000;
}

.exportContainer {
    width: 88%;
    height: calc(100% / 3);
    position: relative;
    border-bottom-style: Solid;
    border-color: rgba(102, 102, 102, 0.5);
    border-width: 1px;
    margin-left: 6%;
    display: flex;
    align-items: center;
}

.logOutContainer {
    width: 88%;
    height: calc(100% / 3);
    position: relative;
    margin-left: 6%;
    display: flex;
    align-items: center;
}

.exportImage {
    margin-left: 6%;
    margin-right: 8%;
}

.exportExcelImage {
    background: url('../Images/CBL Global_sprite_V4.png') no-repeat -1246px 0;
    width: 20px;
    height: 20px;
}

.exportPPTImage {
    background: url('../Images/CBL Global_sprite_V4.png') no-repeat -1343px 0;
    width: 19px;
    height: 21px;
}

.logOutImage {
    background: url('../Images/CBL Global_sprite_V4.png') no-repeat -1439px 0;
    width: 23px;
    height: 18px;
}

.exportText {
    font-family: Montserrat-Light;
    font-size: 0.625em;
    color: #FFFFFF;
}

.exportExcelContainer:hover .exportExcelImage {
    background: url('../Images/CBL Global_sprite_V4.png') no-repeat -1295px 0;
}

.exportPPTContainer:hover .exportPPTImage {
    background: url('../Images/CBL Global_sprite_V4.png') no-repeat -1391px 0;
}

.logOutContainer:hover .logOutImage {
    background: url('../Images/CBL Global_sprite_V4.png') no-repeat -1491px 0;
}

.logOutContainer:hover .exportText {
    font-family: Montserrat-Regular;
}

.exportContainer:hover .exportText {
    font-family: Montserrat-Regular;
}

.TranslucentDiv {
    background-color: lightgrey;
    height: 100%;
    opacity: 0.4;
    width: 100%;
    z-index: 100000001;
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    margin: auto;
}

#Loader {
    bottom: 0;
    height: 100px;
    left: 0;
    margin: auto;
    position: fixed;
    right: 0;
    top: 0;
    width: 101px;
    z-index: 10000000000;
    background: #ffffff;
    border-radius: 10px;
}

.alert-div {
    position: fixed;
    left: 0;
    right: 0;
    top: 40%;
    bottom: 0;
    margin: auto;
    width: 392px;
    font-family: Montserrat-Medium;
    color: #383838;
    z-index: 1000000000;
}

.alert-header {
    background: #FFFFFF;
    height: 35px;
    border-radius: 7px 7px 0px 0px;
    border-bottom: 1px Solid #EA1F2A;
}

.alert-body {
    background: #FFFFFF;
    height: 10.125em;
    border-radius: 0px 0px 7px 7px;
    box-shadow: 0px 0px 5px 0px rgba(0,0,0,0.11);
    position: relative;
}

.alert-ok {
    background: #232323;
    width: 59px;
    height: 26px;
    box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.5);
    border-top: 1px Solid #EA1F2A;
    top: 59%;
    left: 43%;
    position: absolute;
    font-family: Montserrat-Bold;
    font-size: 14px;
    color: #FFFFFF;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
}

.alert-close {
    background: url('../Images/CBL Global_sprite_V5.png') no-repeat -1058px -87px;
    width: 41px;
    height: 35px;
    float: right;
    cursor: pointer;
}

.alert-header-text {
    font-size: 0.938em;
    display: flex;
    align-items: center;
    justify-content: center;
    float: left;
    height: 100%;
    width: calc(100% - 42px);
}

.alert-message {
    font-size: 15px;
    text-align: center;
    position: absolute;
    top: 20%;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 392px;
}

.redirectIcon {
    background: linear-gradient(0deg, rgba(0, 0, 0, 1) 0%, rgba(100, 100, 100, 1) 100%);
    width: 15.625em;
    height: 2.563em;
    cursor: pointer;
    border-radius: 12px;
    box-shadow: 2px 2px 5px rgba(0, 0, 0, 0.30);
    float: right;
    display: flex;
    align-items: center;
    text-align: center;
    justify-content: center;
    margin-top: 16px;
    font-family: Montserrat-Regular;
    font-size: 0.688em;
    text-transform: uppercase;
    color: #FFFFFF;
}

.defaltdataloding {
    position: absolute;
    left: 42%;
    top: 48%;
    display: none;
    z-index: 100000;
    color: white;
}

.showMenu {
    background-image: url(../Images/Icons/showmenu.png);
    position: relative;
    background-size: 3vw 3vw;
    background-repeat: no-repeat;
    top: 4vw;
    left: 1.5vw;
    width: 7vw;
    height: 7vw;
    z-index: 100000001;
    display: none;
}

#search {
    margin-left: 15px;
    padding-left: 10px;
    background: #303030;
    border-style: Solid;
    border-color: #000000;
    border-width: 0.5px;
    border-radius: 11px;
    width: 160px;
    height: 22px;
    padding-right: 1.4vw;
    padding-right:1.4vw!important; 
}

    #search:focus {
        outline: none;
    }

.txt-search {
    width: 90%;
    float: left;
    border: 0;
    background-color: transparent;
    height: 100%;
    font-family: Montserrat-Light;
    font-weight: lighter;
    font-size: 12px;
    color: rgba(255, 255, 255, 0.6);
}

.img-search {
    background-image: url('../Images/CBL Global_sprite_V10.png');
    background-repeat: no-repeat;
    background-position: -1169px -86px;
    float: left;
    height: 10px;
    width: 10px;
    margin-left: -15px;
    margin-top: 5px;
}

.Search-Filter {
    width: 100%;
    float: left;
}

    .Search-Filter .ui-widget-content {
        background-color: #333333;
        border: 1px solid lightgrey;
        color: #444444;
        max-height: 300px;
        min-height: 20px;
        overflow-x: hidden;
        overflow-y: auto;
        padding: 4px 0px;
        margin: 0px 0px 10px 25px;
        list-style: none;
        border-color: rgba(0, 0, 0, 0.2);
        border-style: solid;
        border-width: 1px;
        border-radius: 5px;
        box-shadow: rgba(0, 0, 0, 0.2) 0px 5px 10px;
        -webkit-background-clip: padding-box;
        background-clip: padding-box;
        max-height: 150px;
        color: rgba(255, 255, 255, 0.6);
        overflow: hidden;
        outline: none;
        display: block;
        width: 204px;
        top: 61px;
        left: 17px;
        max-width: 196px;
        overflow-y: scroll;
        font-family: Montserrat-Light;
        position: absolute;
        z-index: 99999;
        font-size: 12px;
    }
